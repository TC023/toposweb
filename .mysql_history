SELECT\040od_inventario.nombreProducto,\040od_ventaProducto.cantidad\040from\040od_inventario,\040od_ventaProducto,\040od_ventas,\040od_descuentoDpt,\040od_descuentoProducto
\040\040\040\040CASE
\040\040\040\040\040\040\040\040WHEN\040(od_inventario.id_departamento\040=\040od_departamento.id_departamento\040AND\040od_inventario.id_producto\040NOT\040IN\040od_descuentoProducto)
\040\040\040\040\040\040\040\040AND\040(od_inventario.id_producto\040NOT\040IN\040od_descuentoProducto.id_producto)\040THEN\040od_inventario.precio_unit*od_ventaProducto.cantidad
\040\040\040\040\040\040\040\040END\040AS\040Total
\040\040\040\040WHERE\040od_ventaProducto.id_producto\040=\040od_inventario.id_producto
\040\040\040\040AND\040od_ventas.id_venta\040=\040od_ventaProducto.id_venta;
