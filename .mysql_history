telefono\040VARCHAR(10)
);
CREATE\040TABLE\040reto_responsables\040(\040id_responsable\040INT\040NOT\040NULL\040PRIMARY\040KEY,\040nombre\040VARCHAR(20),\040correo\040VARCHAR(20),\040telefono\040VARCHAR(10)\040);
CREATE\040TABLE\040reto_eventos\040(
id_evento\040INT\040NOT\040NULL\040PRIMARY\040KEY,
fecha_hora\040TIMESTAMP,
duracion\040INT,
id_tipo\040INT,
id_responsable\040INT,
FOREIGN\040KEY\040(id_tipo)\040REFERENCES\040reto_tipo_evento(id_tipo),
FOREIGN\040KEY\040(id_responsbale)\040REFERENCES\040reto_responsables(id_responsable),
);
