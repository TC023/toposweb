describe\040reto_equipos;
SELECT\040reto_equipos.nombre,\040reto_integrantes_equipos\040FROM\040reto_equipos,\040reto_integrantes_equipos;
SELECT\040reto_equipos.nombre,\040reto_integrantes_equipos.\040FROM\040reto_equipos,\040reto_integrantes_equipos;
select\040*\040from\040reto_integrantes_equipos;
SELECT\040reto_equipos.nombre,\040reto_integrantes_equipos.nombre\040FROM\040reto_equipos,\040reto_integrantes_equipos;
SELECT\040reto_equipos.nombre,\040reto_integrantes_equipos.nombre\040FROM\040reto_equipos,\040reto_integrantes_equipos,\040reto_roles
WHERE\040reto_integrantes_equipos.id_equipo\040=\040reto_equipos.id_equipo;
SELECT\040reto_equipos.nombre,\040reto_integrantes_equipos.nombre\040FROM\040reto_equipos,\040reto_integrantes_equipos,\040reto_roles\040WHERE\040reto_integrantes_equipos.id_equipo\040=\040reto_equipos.id_equipo;
select\040*\040from\040reto_equipos;
select\040*\040from\040reto_integrantes_equipos;
