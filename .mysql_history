SELECT\040s.name_superhero\040AS\040name,
\040\040\040\040\040\040\040i.name_identity\040AS\040identity,
\040\040\040\040\040\040\040p.name_publisher\040AS\040publisher,
\040\040\040\040\040\040\040o.name_other_name\040AS\040other_name,
\040\040\040\040\040\040\040i.first_appearance
